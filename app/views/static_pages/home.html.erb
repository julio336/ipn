<script type="text/javascript">
	$('.link1').click(function(e){
        $('html,body').scrollTo(this.hash, this.hash);
        e.preventDefault();
    });
    $('.link2').click(function(e){
        $('html,body').scrollTo(this.hash, this.hash);
